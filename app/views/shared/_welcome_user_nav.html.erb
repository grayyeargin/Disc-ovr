<div class="row">
	<div class="col-xs-7">
		<div class="navbar-header">
		<form action="/" method="get">
			<input type="submit" class="navbar-brand" value="MusicMap">
		</input>
	</div>
	<div class="col-xs-2 navbar-links view-profile">
		<%= link_to "View Profile", current_user, method: :get %>
